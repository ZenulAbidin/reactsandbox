{
  "name": "react",
  "version": "0.0.0",
  "private": true,
  "dependencies": {
    "eslint": >= "4.18.2",
    "eslint-plugin-react-hooks": "^3.0.0",
    "prop-types": "latest",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "react-is": ">= 16.8.0",
    "styled-components": "latest"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "react-scripts": "latest"
  },
  "eslintConfig": {
    "plugins": [
